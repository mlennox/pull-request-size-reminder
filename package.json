{
  "name": "pull-request-size-reminder",
  "version": "1.0.8",
  "description": "CLI to check potential size of pull request",
  "keywords": [
    "git",
    "pull-request",
    "pull request"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "./src/check.js",
  "files": [
    "src/**/*.js",
    "!src/**/*.test.js"
  ],
  "scripts": {
    "test": "jest .",
    "precommit": "npx eslint . && node ./src/check.js",
    "prepush": "npm test && node ./src/check.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlennox/pull-request-size-reminder.git"
  },
  "author": {
    "name": "Mark Lennox",
    "email": "mark@webpusher.ie",
    "url": "https://webpusher.ie"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlennox/pull-request-size-reminder/issues"
  },
  "homepage": "https://github.com/mlennox/pull-request-size-reminder#readme",
  "dependencies": {
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/jest": "^24.0.13",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "prettier": "^1.17.1"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}